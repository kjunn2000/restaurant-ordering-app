alStorageService.getRole() == \"ROLE_CUSTOMER\") history.push(\"/menu\");\r\n    else if (localStorageService.getRole() == \"ROLE_STAFF\")\r\n      history.push(\"/dashboard\");\r\n    else if (localStorageService.getRole() == \"ROLE_ADMIN\")\r\n      history.push(\"/add-menu\");\r\n  };\r\n\r\n  return (\r\n    <div className=\"loginModal\">\r\n      <Modal show={show} onHide={handleClose}>\r\n        <Modal.Header style={{ alignItems: \"center\" }} closeButton>\r\n          <h3\r\n            className=\"header\"\r\n            style={{ fontSize: \"20pt\", fontFamily: \"serif\" }}\r\n          >\r\n            Login To The MOODY\r\n          </h3>\r\n        </Modal.Header>\r\n        <Modal.Body>\r\n          {\" \"}\r\n          <Form>\r\n            <Form.Group controlId=\"formBasicUsername\">\r\n              <Form.Label>Username</Form.Label>\r\n              <Form.Control\r\n                type=\"text\"\r\n                placeholder=\"Enter username\"\r\n                onChange={handleChange}\r\n                name=\"username\"\r\n              />\r\n              <Form.Text className=\"text-muted\">\r\n                If you forget your username or password please email us.\r\n                <br />\r\n                <b>moodycafe@gmail.com</b>\r\n              </Form.Text>\r\n            </Form.Group>\r\n\r\n            <Form.Group controlId=\"formBasicPassword\">\r\n              <Form.Label>Password</Form.Label>\r\n              <Form.Control\r\n                type=\"password\"\r\n                placeholder=\"Password\"\r\n                onChange={handleChange}\r\n                name=\"password\"\r\n              />\r\n            </Form.Group>\r\n\r\n            <Form.Text\r\n              className=\"text-danger font-weight-bold\"\r\n              id=\"errMessage\"\r\n              ref={errMessage}\r\n              style={{ fontSize: \"10pt\" }}\r\n            >\r\n              ** Please provide your name and password. **\r\n            </Form.